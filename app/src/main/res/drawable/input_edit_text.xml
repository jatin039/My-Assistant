<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="60dp" />
            <stroke
                android:width="1dp"
                android:color="#DDDDDD" />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:angle="270"
                android:endColor="@color/editTextStartColor"
                android:startColor="@color/editTextStartColor" />
            <stroke
                android:width="1dp"
                android:color="@color/editTextStartColor" />
            <corners
                android:radius="60dp" />
            <padding
                android:bottom="5dp"
                android:left="20dp"
                android:right="3dp"
                android:top="5dp" />
        </shape>
    </item>


</layer-list>